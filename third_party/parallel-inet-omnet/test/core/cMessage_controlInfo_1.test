%description:
Test that getControlInfo() gets deleted when message gets deleted

%global:

class Ctrl : public cPolymorphic
{
  public:
    Ctrl() : cPolymorphic() { }
    ~Ctrl() {ev << "control info deleted\n";}
};

%activity:
cMessage *msg = new cMessage();
Ctrl *ctrl = new Ctrl();

msg->setControlInfo(ctrl);
delete msg;

%contains: stdout
control info deleted

