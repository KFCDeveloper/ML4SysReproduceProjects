%description:
check that cLinkedList::operator=() works correctly and preserves order

%module: Llist

int one = 1;
int two = 2;
int three = 3;

class Llist : public cSimpleModule
{
  public:
    Llist() : cSimpleModule(16384) { }
    virtual void activity();
    void fillLlist(cLinkedList& ll);
    void output(cLinkedList& ll);
};

Define_Module(Llist);

void Llist::fillLlist(cLinkedList& ll)
{
  ll.insert(&one);
  ll.insert(&three);
  ll.insert(&two);
}

void Llist::output(cLinkedList& ll)
{
  ev << ll.getName() << ":";
  while (!ll.isEmpty())
    ev << " " << *((int*) ll.pop());
  ev << endl;
}

void Llist::activity()
{
  cLinkedList initial("Initial");
  cLinkedList duplicate("Duplicate");

  fillLlist(initial);
  duplicate = initial;
  output(initial);
  output(duplicate);
}

%contains: stdout
Initial: 1 3 2
Duplicate: 1 3 2
